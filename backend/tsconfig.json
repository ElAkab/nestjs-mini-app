{
	"compilerOptions": {
		"useDefineForClassFields": false,
		"module": "Node16",
		"moduleResolution": "node16",
		"target": "ES2023",
		"strict": true,
		"esModuleInterop": true,
		"declaration": true,
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"outDir": "./dist",
		"rootDir": "./",
		"skipLibCheck": true,
		"incremental": true
	}
}
