{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": true,
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"target": "ES2024",
		"sourceMap": true,
		"outDir": "./dist",
		"rootDir": "./",
		"baseUrl": "./src",
		"skipLibCheck": true,
		"incremental": true
	}
}
